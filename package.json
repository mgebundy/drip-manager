  "keywords": [
    "drip",
    "drip.fm",
    "music"
  ],
  "license": "MIT",
  "author": {
    "name": "Mitchell Bundy",
    "email": "mitch@bundy.ca",
    "url": "http://www.bundy.ca/"
  },
  "repository": "mgebundy/drip-manager",
  "preferGlobal": true,
